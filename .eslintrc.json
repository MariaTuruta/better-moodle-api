{
    "extends": "airbnb-base",
    "rules": {
      "linebreak-style": [
        "off",
        "windows"
      ],
      "no-unused-vars": "warn",
      "global-require": "warn",
      "no-use-before-define": "warn",
      "arrow-body-style": "off",
      "strict": "off",
      "complexity": [
        "error",
        4
      ]
    }
}
